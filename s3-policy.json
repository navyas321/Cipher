{
  "Version": "2012-10-17",
  "Statement": [
    {
      "Effect": "Allow",
      "Action": [
        "s3:GetObject",
        "s3:HeadObject",
        "s3:ListBucket",
        "s3:CreateBucket",
        "s3:PutBucketVersioning"
      ],
      "Resource": [
        "arn:aws:s3:::my-video-lambda-bucket",
        "arn:aws:s3:::my-video-lambda-bucket/*",
        "arn:aws:s3:::*"
      ]
    }
  ]
}
